{
  "name": "@nokken65/uikit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm dev:ladle\" \"pnpm dev:scss\"",
    "dev:ladle": "ladle serve",
    "dev:scss": "pnpm build:scss --watch",
    "build": "node ./scripts/build.mjs",
    "build:scss": "typed-scss-modules src --nameFormat camel",
    "build:ladle": "ladle build",
    "preview": "ladle preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix"
  },
  "devDependencies": {
    "@types/node": "^18.7.13",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "clsx": "^1.2.1",
    "concurrently": "^7.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "globby": "^13.1.2",
    "postcss": "^8.4.16",
    "postcss-import": "^14.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.5",
    "tailwindcss": "^3.1.8",
    "typed-scss-modules": "^6.5.0",
    "typescript": "4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.4.1"
  },
  "peerDependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0"
  },
  "dependencies": {
    "@ladle/react": "^2.2.2"
  }
}
